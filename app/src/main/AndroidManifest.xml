<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.medicine_tracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_icon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Medicine_Tracker">
        <activity
            android:name=".Splash_Activity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Show_Prescription"
            android:exported="false" />
        <activity
            android:name=".MedicineViewActivity"
            android:exported="false" />
        <activity
            android:name=".MedicineActivity3"
            android:exported="false" />
        <activity
            android:name=".MedicineActivity2"
            android:exported="false" />
        <activity
            android:name=".DailyActivity3"
            android:exported="false" />
        <activity
            android:name=".DailyActivity2"
            android:exported="false" />
        <activity
            android:name=".DoctorActivity3"
            android:exported="false" />
        <activity
            android:name=".DoctorActivity2"
            android:exported="false" />
        <activity
            android:name=".DoctorActivity"
            android:exported="false" />
        <activity
            android:name=".NotificationActivity"
            android:exported="false" />
        <activity
            android:name=".MedicineActivity"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".DailyActivity"
            android:exported="true" />
        <activity
            android:name=".HomePage"
            android:exported="true">

        </activity>

        <receiver android:name=".AlarmReceiver" />
        <receiver android:name=".AlarmReceiver_day" />
        <receiver android:name=".AlarmReceiver_night" />
    </application>

</manifest>